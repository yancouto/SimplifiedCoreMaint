{
    "configurations": [
        {
            "name": "Launch m4 fb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\core-maint\\debug.exe",
            "args": ["-p", "experiment/graph/facebook_combined.txt", "-I", "100", "-m", "4"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\msys64\\mingw64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}